<div id="event-view">
  <h3><em><%= event.name %></em></h3>
  <h4>Description: <%= event.description %></h4>
  <h4>Date: <%= event.date %></h4>
  <h5>Time: <%= event.start_time %> - <%= event.end_time %></h5><br>
  Address: <%= event.address_line_1 %><br>
     <%= event.address_line_2 %><br>
     <%= event.city %><br>
     <%= event.zip %><br>
     <%= event.country %>
  <p>
    <%= link_to "Edit", edit_wedding_event_path(event.wedding, event) %> |
    <%= link_to "Delete", wedding_event_path(event.wedding, event), method: :delete, data: {confirm: "Are you sure you want to delete this Event?"} %>
  </p>
</div>