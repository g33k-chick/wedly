<h2>All Registries for <%= @wedding.partner_1 %> &#38 <%= @wedding.partner_2 %></h2>

<% @registries.each do |registry| %>
  <p><strong>Store: </strong><%= registry.store %></p>
  <p><strong>Link: </strong><%= registry.link_to_registry %></p>
  <p><%= registry.image_url %></p>

<% end %>

<%= form_for @wedding do |f| %>
<div id= "links">
    <%= link_to_add_association 'add registry', f, :registries %>

    <% f.submit %>

    <% end %>
  </div>