<div class = "playlist_container">
  <% if current_user == @wedding.user %>
    <%=form_for([@playlist.wedding,  @playlist])do |f| %>
      <div class = "nested-fields">
        <%= f.hidden_field :wedding_id, value: @wedding.id %>
        <div class= "submit-button">
          <%= f.submit %>
        </div>
      </div>
    <% end %>
  <% end %>
</div>