<h1>All Events for "<%= @wedding %>"</h1>

<% @events.each do |event| %>
  <h3><%= event.name %></h3>
  <h4><%= event.date %></h4>
  <h5><%= event.start_time%> - <%= event.end_time %></h5>
  <p><%= event.address_line_1 %></p>
  <p><%= event.address_line_2 %></p>
  <p><%= event.city %></p>


<br/>
<br/>
<% end %>






