<div class="song_container">
  <div class="song_form">
    <div class="form">
       <%= form_for @song do |f| %>

        <div class="field">
          <%= f.label :title %><br>
          <%= f.text_field :title %>
        </div>
        <br/>
         <div class="field">
          <%= f.label :artist %><br>
          <%= f.text_field :artist %>
        </div>
        <div class="actions">
          <%= f.submit %>
        </div>
      <% end %>
    </div>
  </div>
</div>
